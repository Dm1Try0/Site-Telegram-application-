<body>


	<div>
		<div class="swiper-wrapper" style="transform: translate3d(0px, 0px, 0px); transition-duration: 0ms">
			@if (ViewBag.premium == "true")
			{
				@foreach (string item in ViewBag.titleinfo)
				{
					string[] b = item.Split(":");
					<div class="swiper-slide"><div class="new-photo-block-item"><a href="/Home/ItemPage/?id=@($"{b[1]}")&premium=true" class="new-photo-block-item-img"><img src="/images/@($"{b[3]}")"></a><a href="/Home/ItemPage/?id=@($"{b[1]}")&premium=true" class="new-photo-block-item-name">@($"{b[0]}")</a></div></div>
					<br />
				}
			}
			@if (ViewBag.premium != "true")
			{
				@foreach (string item in ViewBag.titleinfo)
				{
					string[] b = item.Split(":");
					<div class="swiper-slide"><div class="new-photo-block-item"><a href="/Home/ItemPage/?id=@($"{b[1]}")" class="new-photo-block-item-img"><img src="/images/@($"{b[3]}")"></a><a href="/Home/ItemPage/?id=@($"{b[1]}")" class="new-photo-block-item-name">@($"{b[0]}")</a></div></div>
					<br />
				}
			}
			

			</div>
	</div>
	<br>
	@if (ViewBag.premium == "true")
	{
		<div style="display:inline-flex;width: 100%;">
			<span class="category">Выберите категорию</span>
			<a href="/Home/All?premium=true" style="padding-top:7.5px;color: var(--tg-theme-hint-color);position: absolute;display:flex;right: 10px;">Показать всех</a>
		</div>
	}
	@if (ViewBag.premium != "true")
		{
			<div style="display:inline-flex;width: 100%;">
		<span class="category">Выберите категорию</span>
		<a href="/Home/All" style="padding-top:7.5px;color: var(--tg-theme-hint-color);position: absolute;display:flex;right: 10px;">Показать всех</a>
	</div>
		}
	
	@if (ViewBag.premium == "true")
	{
		<div class="top-links">
			<a href="/Home/All?tag=tag1&premium=true" class="top-links__link"><img src="https://i.imgur.com/x5u25nb.png" class="top-links__link-icon"><span class="top-links__link-text">Категория1</span></a>

			<a href="/Home/All?tag=tag2&premium=true" class="top-links__link"><img src="https://i.imgur.com/luvJsrr.png" class="top-links__link-icon"><span class="top-links__link-text">Категория2</span></a>

			<a href="/Home/All?tag=tag3&premium=true" class="top-links__link"><img src="https://i.imgur.com/n5wXvFJ.png" class="top-links__link-icon"><span class="top-links__link-text">Категория3</span></a>

			<a href="/Home/All?tag=tag4&premium=true" class="top-links__link"><img src="https://i.imgur.com/UrG919d.png" class="top-links__link-icon"><span class="top-links__link-text">Категория4</span></a>

			<a href="/Home/All?tag=tag5&premium=true" class="top-links__link"><img src="https://i.imgur.com/b2o8NlF.png" class="top-links__link-icon"><span class="top-links__link-text">Категория5</span></a>

			<a href="/Home/All?tag=tag6&premium=true" class="top-links__link"><img src="https://i.imgur.com/yrvRJLF.png" class="top-links__link-icon"><span class="top-links__link-text">Категория6</span></a>
	</div>
	}
	@if (ViewBag.premium != "true")
