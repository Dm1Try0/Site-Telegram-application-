    <div>
			
       
				
			
            <div class="tab_container">
		<div id="tab1" class="tab_content" style="display:block">
			<div id="allEntries" class="top main Photo">
            <ul id="uEntriesList" class="allEntriesTable u-ecc-2">
            
			@foreach (IndexPage item in Model)
			{
								if(ViewBag.premium == "true")
								{
									<li class="phtTdMain uEntryWrap">
										<div class="h-photo-mtr">
											<a href="/Home/ItemPage?id=@item.Id&premium=true">
												<img src="/images/@($"{item.photoTitle}")">
												<div class="h-photo-block-post-button">
													@($"{item.Name}")
												</div>
												<span class="site_usser">@($"{item.pageCount}") фото</span>
											</a>
										</div>
									</li>
								}
								if (ViewBag.premium != "true")
								{
									<li class="phtTdMain uEntryWrap">
										<div class="h-photo-mtr">
											<a href="/Home/ItemPage?id=@item.Id">
												<img src="/images/@($"{item.photoTitle}")">
												<div class="h-photo-block-post-button">
													@($"{item.Name}")
												</div>
												<span class="site_usser">@($"{item.pageCount}") фото</span>
											</a>
										</div>
									</li>
								}
			
			}
            
            </ul>
                    </div>
                </div>
            </div>

			<center>
				@if (ViewBag.premium == "true")
				{
					@for (int i = 1; i < ViewBag.pagecounts; i++)
					{
						<a href="/Home/All?page=@($"{i}")&premium=true" class="pager">@($"{i}")</a>
					}
				}
				@if(ViewBag.premium != "true")
				{
					@for (int i = 1; i < ViewBag.pagecounts; i++)
					{
						<a href="/Home/All?page=@($"{i}")" class="pager">@($"{i}")</a>
					}
				}
				
			</center>
    
    </div>
</div>
